# Playwright Test Environment Variables
# Copy this file to .env.test and fill in your test credentials

# Test user credentials for authentication
# Use a dedicated test account for Playwright tests
PLAYWRIGHT_TEST_EMAIL=hi@skillvee.com
PLAYWRIGHT_TEST_PASSWORD=Skillveetest

# Alternative: Use Clerk testing tokens (if available)
# CLERK_TESTING_TOKEN=your-clerk-testing-token

# Set to 'true' to run tests in headed mode for debugging
PLAYWRIGHT_HEADED=false

# Set to 'true' to enable slower operations for debugging
PLAYWRIGHT_SLOW_MO=false